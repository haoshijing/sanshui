<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.keke.sanshui.base.admin.dao.HeadAgentDAO">
    <sql id="all_columns">
        *
    </sql>
    <sql id = "tableName">
        t_head_agent
    </sql>
    <select id="selectList" resultType="HeadAgentPo">
        <!-- 具体的sql -->
        SELECT <include refid="all_columns"/> FROM
        <include refid="tableName"/>

        <where>
            <if test = "headAgentName != null and headAgentName != ''">
                headAgentName = #{headAgentName}
            </if>
        </where>
    </select>


</mapper>